//model render() method which return html that control the web interface; such code can be runed in react, I have no idea how it should be used in real web development yet
import React from 'react';

class Greet extends React.Component {
	
	constructor(props){
		super(props);
		
		this.state = {val: "awesome"};
	}
	
	
	changeValue = () => {
		this.setState(
			{val: "wonderful"}
			);
		}
	
	render(){
		return(
			<div>
				<h1>Hello {this.state.val} World</h1>
				<button type="button" onClick={this.changeValue}>Change value</button>
			</div>
		);
	}
}


export default Greet;
